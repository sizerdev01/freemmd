local vis = PVPTab:AddSection({
    Name = "Visual"
})

vis:AddToggle({
    Name = "Pegar itens(precisa revistar)",
    Default = false,
    Callback = function(state)
        if state then
            -- Ativa o script
            visScriptThread = coroutine.create(function()
                while true do
                    pcall(function()
                        loadstring(game:HttpGet("https://raw.githubusercontent.com/devercrate/caclol/refs/heads/main/revisstar"))()
                    end)
                    wait(1) -- Ajuste o intervalo conforme necess√°rio
                end
            end)
            coroutine.resume(visScriptThread)
        else
            -- Para o script interrompendo a execu√ß√£o da thread
            if visScriptThread and coroutine.status(visScriptThread) == "suspended" then
                coroutine.close(visScriptThread) -- Fecha a thread
            end
        end
    end
})

local TeleportTab = Window:MakeTab({
    Name = "Teleports",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

------------------------------ DROPDOWNES
local PlayerDropdown, teleportDropdown

-- Fun√ß√£o para atualizar o Dropdown de visualiza√ß√£o de itens do backpack
local function updatePlayerDropdown()
    if PlayerDropdown then
        local playerList = {}
        for _, player in ipairs(players:GetPlayers()) do
            table.insert(playerList, player.Name)
        end
        PlayerDropdown:Refresh(playerList, true) -- Atualiza o dropdown mantendo a sele√ß√£o atual
    end
end

-- Fun√ß√£o para atualizar o Dropdown de teleporte
local function updateTeleportDropdown()
    if teleportDropdown then
        local playerList = {}
        for _, player in ipairs(players:GetPlayers()) do
            if player ~= players.LocalPlayer then
                table.insert(playerList, player.Name)
            end
        end
        teleportDropdown:Refresh(playerList, true) -- Atualiza o dropdown mantendo a sele√ß√£o atual
    end
end

-- Atualiza os Dropdowns ao in√≠cio e em eventos de entrada/sa√≠da de jogadores
players.PlayerAdded:Connect(function()
    updatePlayerDropdown()
    updateTeleportDropdown()
end)

players.PlayerRemoving:Connect(function()
    updatePlayerDropdown()
    updateTeleportDropdown()
end)

local function viewPlayer(player)
    local character = player.Character or player.CharacterAdded:Wait()
    if character and character:FindFirstChild("Head") then
        camera.CameraSubject = character
        OrionLib:MakeNotification({
            Name = "Vis√£o ativada",
            Content = "Voc√™ est√° agora vendo " .. player.Name,
            Time = 5
        })
    else
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "N√£o foi poss√≠vel visualizar o jogador.",
            Time = 5
        })
    end
end

local function unviewPlayer()
    camera.CameraSubject = Players.LocalPlayer.Character -- Volta para o pr√≥prio jogador
    OrionLib:MakeNotification({
        Name = "Vis√£o desativada",
        Content = "Voc√™ parou de ver o jogador.",
        Time = 5
    })
end

local viewDropdown

local function updateViewPlayerList()
    local playerNames = {}
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= Players.LocalPlayer then
            table.insert(playerNames, player.Name)
        end
    end

    if viewDropdown then
        viewDropdown:Refresh(playerNames, true)  -- Atualiza as op√ß√µes do dropdown com refresh
    else
        viewDropdown = vis:AddDropdown({
            Name = "View player",
            Default = "",  -- Valor padr√£o vazio
            Options = playerNames,
            Callback = function(selectedPlayerName)
                if selectedPlayerName ~= "" then
                    local player = Players:FindFirstChild(selectedPlayerName)
                    if player then
                        viewPlayer(player)
                    end
                end
            end
        })
    end
end

updateViewPlayerList()

Players.PlayerAdded:Connect(updateViewPlayerList)
Players.PlayerRemoving:Connect(updateViewPlayerList)

vis:AddButton({
    Name = "Unview",
    Callback = function()
        unviewPlayer()
    end
})

local scriptConnection

-- Fun√ß√£o para carregar o script
local function loadScript()
    scriptConnection = loadstring(game:HttpGet("https://raw.githubusercontent.com/devercrate/caclol/refs/heads/main/veritens"))()
end

-- Fun√ß√£o para exibir a notifica√ß√£o e carregar o script ao pressionar o bot√£o
vis:AddButton({
    Name = "Ver itens UI",
    Callback = function()
        loadScript()
        OrionLib:MakeNotification({
            Name = "Ver itens UI",
            Content = "UI ativada com sucessso",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})


---------------------- VIEW ACIMA --------------------
PlayerDropdown = vis:AddDropdown({
    Name = "View equipped items",
    List = {},
    Default = nil,
    Callback = function(SelectedPlayer)
        local player = players:FindFirstChild(SelectedPlayer)
        if player then
            local backpackItems = {}
            for _, item in pairs(player.Backpack:GetChildren()) do
                table.insert(backpackItems, item.Name)
            end
            local itemsText = table.concat(backpackItems, ", ")
            local notificationContent = #backpackItems > 0 and "Items: " .. itemsText or "Sem itens equipados"
            OrionLib:MakeNotification({
                Name = player.Name .. "'s Itens Equipados",
                Content = notificationContent,
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
})

-- Definir configura√ß√µes da HealthBar
getgenv().Settings = {
    HealthBar_Enabled = false,     -- Ativar/Desativar HealthBar
    HealthBar_Color = Color3.fromRGB(0, 255, 0), -- Cor da Barra de Sa√∫de (Verde)
}

-- Fun√ß√£o para ativar/desativar a HealthBar
local function ToggleHealthBar()
    getgenv().Settings.HealthBar_Enabled = not getgenv().Settings.HealthBar_Enabled
    if getgenv().Settings.HealthBar_Enabled then
        -- Carregar o script da healthbar do reposit√≥rio quando ativar
        loadstring(game:HttpGet("https://raw.githubusercontent.com/sizerdev01/mdm.menu/refs/heads/main/healthbar"))()
        print("HealthBar ativada!")
    else
        -- Caso contr√°rio, a healthbar ser√° desativada e podemos manipular isso.
        print("HealthBar desativada!")
    end
end

-- Bot√£o para ativar/desativar a HealthBar
vis:AddButton({
    Name = "Ativar helthbar/ irreversivel",
    Callback = ToggleHealthBar
})

-- AQUI COME√áA A L√ìGICA DE ESP COM A HEALTHBAR

-- Verificar a cada `RenderStepped` se a HealthBar est√° ativa ou n√£o
game:GetService("RunService").Heartbeat:Connect(function()
    if getgenv().Settings.HealthBar_Enabled then
        -- A healthbar est√° ativa e foi carregada atrav√©s do toggle
        -- O c√≥digo do ESP j√° est√° dentro do loadstring, n√£o precisamos recriar aqui.
    else
        -- A healthbar est√° desativada, apenas monitore o estado.
    end
end)

local isESPNameActive = false
vis:AddToggle({
    Name = "ESP Name",
    Default = false,
    Callback = function(Value)
        isESPNameActive = Value
        if isESPNameActive then
            monitorAllPlayersESPName()
            print("ESP Name ativado ")
        else
            for _, player in ipairs(players:GetPlayers()) do
                local character = player.Character
                local espName = character and character:FindFirstChild("ESPName")
                if espName then
                    espName:Destroy()
                end
            end
            print("ESP Name desativado")
        end
    end
})

local isHighlightActive = false
vis:AddToggle({
    Name = "ESP",
    Default = false,
    Callback = function(Value)
        isHighlightActive = Value
        if isHighlightActive then
            monitorAllPlayersESP()
            print("ESP ativado")
        else
            for _, player in ipairs(players:GetPlayers()) do
                local character = player.Character
                local highlight = character and character:FindFirstChildOfClass("Highlight")
                if highlight then
                    highlight:Destroy()
                end
            end
            print("ESP desativado")
        end
    end
})

teleportDropdown = TeleportTab:AddDropdown({
    Name = "Teleport Player",
    List = {},
    Default = nil,
    Callback = function(SelectedPlayer)
        local player = players:FindFirstChild(SelectedPlayer)
        if player then
            local localPlayer = players.LocalPlayer
            if player.Character then
                localPlayer.Character:SetPrimaryPartCFrame(player.Character.HumanoidRootPart.CFrame)
                OrionLib:MakeNotification({
                    Name = "Teleportado",
                    Content = "Voc√™ foi teleportado para " .. player.Name,
                    Time = 5
                })
            else
                OrionLib:MakeNotification({
                    Name = "Erro",
                    Content = "N√£o foi poss√≠vel teleportar.",
                    Time = 5
                })
            end
        end
    end
})

-- Inicializa√ß√£o
updatePlayerDropdown()
updateTeleportDropdown()

