TeleportTab:AddToggle({
    Name = "TP Click",
    Default = false,
    Callback = function(value)
        isTPClickActive = value  -- Atualiza o estado do toggle
    end
})

local UIS = game:GetService("UserInputService")
local Player = game.Players.LocalPlayer
local Mouse = Player:GetMouse()

-- Fun√ß√£o que obt√©m o personagem do jogador
function GetCharacter()
   return game.Players.LocalPlayer.Character
end

-- Fun√ß√£o que executa o teleport
function Teleport(pos)
   local Char = GetCharacter()
   if Char then
       Char:MoveTo(pos)
   end
end

-- Monitora quando o bot√£o esquerdo do mouse √© pressionado
UIS.InputBegan:Connect(function(input)
   if isTPClickActive and input.UserInputType == Enum.UserInputType.MouseButton2 then
       Teleport(Mouse.Hit.p)  -- Teleporta o jogador para a posi√ß√£o do clique
   end
end)
