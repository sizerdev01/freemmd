local MiscTab = Window:MakeTab({
    Name = "Misc",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

MiscTab:AddToggle({
    Name = "auto drogas",
    Default = false,
    Callback = function(Value)
        isFarmActive = Value
        if isFarmActive then
            spawn(loopFarm)
            print("Farm ativado")
        else
            print("Farm desativado")
        end
    end
})

MiscTab:AddToggle({
    Name = "Verificar STAFF",
    Default = false,
    Callback = function(Value)
        isCheckActive = Value
        if isCheckActive then
            spawn(startChecking)
            print("Verifica√ß√£o de STAFF ativada")
        else
            print("Verifica√ß√£o de STAFF desativada")
        end
    end
})

